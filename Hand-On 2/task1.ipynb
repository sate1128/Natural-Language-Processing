    "import os\n",
    "source_dir = \"/Users/sate/Desktop/Handon/movie\"\n",
    "for item in os.listdir(source_dir):  # loop through items in dir\n",
    "    if item.endswith(\".srt\"):\n",
    "        f = open(item, 'r', encoding='utf-8')\n",
    "        content = f.read()\n",
    "        f.close()\n",
    "        print(content)"
   ]
